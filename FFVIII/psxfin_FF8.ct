timer_onTimer( Timer, function ( )
local vGameState = readSmallInteger(AddrGameState)
local vIntroState = ReadBytes(AddrIntroState,1)

if DoHealthLock == true then
 SetHealthLockField()
end

if DoFixNames == true then
 --SetFixedNames() --Makes battle name getters use the party formation instead of character values so the names can more easily be changed in the table.
 if ReadSmallInteger(AddrCachedName) == 0x7808 then
  WriteBytes(AddrCachedName,0x6C,0x7E,0x63,0x24,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x21,0x18,0x73,0x00,0x00,0x00)
  WriteBytes(AddrChoiceName,0x07,0x80,0x03,0x3C,0x6C,0x7E,0x63,0x24,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x21,0x20,0x83,0x00,0x10,0x00,0xBF,0xAF,0x00,0x00)
  WriteBytes(AddrChoiceNameTwo,0x00,0x00)
 end
end

if vGameState == 0x3 then --Is in battle

--[[if DoHealthLock == true then
 SetHealthLockBattle()
end--]]

SetModelSize()
SizeOffset = SizeOffset + 1
if SizeOffset == 7 then
 SizeOffset = 0 --Loop back so it updates constantly.
end

if DoATBNerf == true then
 SetNerfedATB()
end

if vIntroState &gt; 0x2 then --Everyone loaded in and battle officially started
if DoPlayableFixes == true then
 DoPlayAsMonsterFixes()
end

if EnemyOffset &lt; 4 then --Hasn't yet looked through all enemy slots
 local ValuEnemyModel = readBytes(AddrEnemy1Model+0x9C*EnemyOffset,1)
 if IntDevourArray[ValuEnemyModel] --table contains this enemy ID
 and DoDevourPatch == true then
  SetDevourEffectAndSound(IntDevourArray[ValuEnemyModel][1],IntDevourArray[ValuEnemyModel][2])
 end

 if EnemyOffset == 0 --Apply devour assembly just once
 and DoDevourAssembly == true then
  writeSmallInteger(AddrDevourHP,0x0000)
  writeSmallInteger(AddrDevourRNG,0x0000)
 end
 EnemyOffset = EnemyOffset + 1 --Increment the offset multiplier for the next enemy slot
end

end

elseif vGameState == 0x2 or vGameState == 0x4 then
 EnemyOffset = 0 --Reset offsets for the next battle
 PlayerOffset = 0
 ATBStarting = false
end

end )
-------------End Timer Function-------------

function SetDevourEffectAndSound(vEffect, vSound)
 AddrEnemySetup = ((ReadInteger(AddrRAMStart+0xF060C+0x34*EnemyOffset) - 0x80000000) + AddrRAMStart) --This is how we grab a 32-bit pointer in a 64-bit emulator
 WriteBytes(AddrEnemySetup+0xFB,vEffect,vEffect,vEffect) --Three tiers of effects
 WriteBytes(AddrEnemySetup+0xFF,vSound)
end

function SetModelSize()
 if BoolPartySize[SizeOffset+1] == true then
  local vCachedSize = ReadSmallInteger(AddrParty1Size+0x9C*SizeOffset)
  if not (vCachedSize == 0x1000*MultPartySize[SizeOffset+1]) and not (vCachedSize == 0x0000) then
   WriteSmallInteger(AddrParty1Size+0x9C*SizeOffset,0x1000*MultPartySize[SizeOffset+1])
  end
 end
end

function DoPlayAsMonsterFixes()
 if PlayerOffset &lt; 3 then
  local ValuPartyModel = readBytes(AddrParty1Model+0x9C*PlayerOffset,1)
  local ValPointerListPointer = ReadInteger(AddrSlotPointerList+0x9C*PlayerOffset)
  if ValPointerListPointer &gt; 0x80000000 then --If slot is valid.
   --Set up the pointer for the AnimSet
   AddrAnimSetData = ((ReadInteger(((ValPointerListPointer - 0x80000000) + AddrRAMStart) + 0x18) - 0x80000000) + AddrRAMStart)
   if FuncAnimSetFix[ValuPartyModel] then --If there's an entry in the AnimSet array
    FuncAnimSetFix[ValuPartyModel]() --Run the indexed function.
   end
   if ReadSmallInteger(AddrAnimSetData+0x07*2) == 0x0000 then --If anim 07 is undefined.
    if ValuPartyModel &gt;= 0x10 then --If slot is enemy.
     WriteSmallInteger(AddrAnimSetData+0x07*2,ReadSmallInteger(AddrAnimSetData+0x0B*2)) --Default it to magic cast.
    else --Slot must be party, then.
     WriteSmallInteger(AddrAnimSetData+0x07*2,ReadSmallInteger(AddrAnimSetData+0x18*2)) --Default it to original devour.
    end
   end
  end
  PlayerOffset = PlayerOffset + 1

  WriteBytes(AddrCommandAnims+0x7,0x07) --Devour to seemingly unused anim. Prolly used somewhere, but...

  if DoGenericCommands == true then
--   WriteBytes(AddrCommandAnims+0x03,0x0E) --Summon needs to be summon.
   WriteBytes(AddrCommandAnims+0x04,0x0B) --Item to magic.
   --WriteBytes(AddrCommandAnims+0x06,0x0B) --Draw-like to magic.
   WriteBytes(AddrCommandAnims+0x0A,0x0B) --Draw to magic.
   --WriteBytes(AddrCommandAnims+0x0D,0x0B) --Item-like to magic.
   --WriteBytes(AddrCommandAnims+0x17,0x0B) --Draw-like to magic.
   WriteBytes(AddrCommandAnims+0x1C,0x0D) --Darkside to attack.
   WriteBytes(AddrCommandAnims+0x1F,0x0B) --Kamikaze to magic.
--   WriteBytes(AddrCommandAnims+0x26,0x0E) --Summon-like needs to be summon.
  end
 end

 if DoOldDevourMethod == true then
  OldDevourMethod()
 end
end

function OldDevourMethod()
 local ValuActionName = readInteger(AddrActionName)
-- local ValuCurrentText = readInteger(AddrCurrentText)
 local ValuActiveSlot = readBytes(AddrActiveSlot,1)
 local ValuActiveModel = readBytes(AddrParty1Model+0x9C*ValuActiveSlot,1)
 local ValuDesiredAnim = readBytes(AddrDesiredAnim+0x40*ValuActiveSlot,1)
 if VarChangedAnim == ValuActiveSlot then
  if ReadSmallInteger(AddrDesiredAnim+0x2E+0x40*ValuActiveSlot) &gt; 0xB000 then --If anim data glitched out.
   WriteSmallInteger(AddrDesiredAnim+0x2E+0x40*ValuActiveSlot,0x0000) --Then fix it.
  end
  if ReadBytes(AddrSomeoneActing,1) == 0 then --If no one's acting after Devour was used,
   WriteBytes(AddrActiveSlot,0xFF) --Invalidate the active slot so the LUA doesn't get confused on who's acting. (I think the game's okay with this)
   VarChangedAnim = 9 --Set no one as using Devour.
  end
 elseif ValuActiveSlot &gt;= 3 then --If an enemy is active.
  VarChangedAnim = 9 --Set no one as using Devour.
 elseif ValuActionName == IntDevourName
 and ValuActiveModel &gt;= 0x10
 and ValuDesiredAnim == 0x1
 then
  if IntDevourAnimArray[ValuActiveModel] then
   WriteBytes(AddrDesiredAnim+0x40*ValuActiveSlot,IntDevourAnimArray[ValuActiveModel])
  else
   WriteBytes(AddrDesiredAnim+0x40*ValuActiveSlot,0x6)
  end
  VarChangedAnim = ValuActiveSlot
 end
end

function SetNerfedATB()
 local ValuPartyATBMax = ReadInteger(AddrParty1ATBMax+0xD0*ATBOffset)
 if ReadBytes(AddrParty1Model+0x9C*ATBOffset,1) &gt;= 0x10
 and ValuPartyATBMax &lt; 0x00005000
 then
  WriteInteger(AddrParty1ATBMax+0xD0*ATBOffset,ValuPartyATBMax*5.46125)
 end

 if ATBStarting == false then
  local ValuCurrentText = ReadInteger(AddrCurrentText)
  if ValuCurrentText == IntFirstStrikeName
  or ValuCurrentText == IntBackAttackName
  then
   WriteInteger(AddrParty1ATBMax+0x4+0xD0*0,0x0FFFFFFF)
   WriteInteger(AddrParty1ATBMax+0x4+0xD0*1,0x0FFFFFFF)
   WriteInteger(AddrParty1ATBMax+0x4+0xD0*2,0x0FFFFFFF)
   ATBStarting = true
  end
 end

 ATBOffset = ATBOffset + 1
 if ATBOffset == 3 then
  ATBOffset = 0
 end
end

